<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="hibernateclass.StudenciUczelnia" table="studenci_uczelnia" schema="public" optimistic-lock="version">
        <id name="studentId" type="string">
            <column name="student_id" length="6" />
            <generator class="assigned" />
        </id>
        <property name="nazwisko" type="string">
            <column name="nazwisko" length="20" />
        </property>
        <property name="imie" type="string">
            <column name="imie" length="15" />
        </property>
        <property name="pesel" type="string">
            <column name="pesel" length="11" />
        </property>
        <property name="dataUr" type="string">
            <column name="data_ur" length="12" />
        </property>
        <property name="miasto" type="string">
            <column name="miasto" length="20" />
        </property>
        <property name="kod" type="string">
            <column name="kod" length="6" />
        </property>
        <property name="ulica" type="string">
            <column name="ulica" length="20" />
        </property>
        <property name="numer" type="string">
            <column name="numer" length="4" />
        </property>
        <property name="tel" type="string">
            <column name="tel" length="9" />
        </property>
        <property name="EMail" type="string">
            <column name="e_mail" length="40" />
        </property>
        <property name="uczelnia" type="string">
            <column name="uczelnia" length="15" />
        </property>
    </class>
</hibernate-mapping>
